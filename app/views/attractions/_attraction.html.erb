<div class="attraction-preview card">
  <a class="attraction-preview-link" href="/search?attraction=<%=attraction.place_id%>"></a>
  <div style="background-image: url(<%= attraction.photo_url %>);" class="city-image attraction-image-container"></div>
  <div class="card-block">
    <h4 class="card-title attraction-preview-name"><%= attraction.name %></h4>
  </div>
</div>
